{
    "name": "phlib/hashstrategy",
    "description": "PHP hash strategy library including random, sorted, consistent",
    "type": "library",
    "homepage": "https://github.com/phlib/hashstrategy",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "James Dempster"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/hashstrategy/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\HashStrategy\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symplify/easy-coding-standard": "^12"
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\HashStrategy\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi"
    }
}
